{
  "name": "kivagant/staticus-core",
  "description": "The core part of Staticus Project. See more in the kivagant/staticus project on the GitHub.",
  "homepage": "https://github.com/KIVagant/staticus-core",
  "keywords": [
    "file",
    "manager",
    "generator",
    "api"
  ],
  "type": "library",
